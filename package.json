{
  "name": "yox",
  "version": "0.63.0",
  "description": "A lightweight mvvm framework",
  "main": "dist/yox.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "dev": "node_modules/.bin/rollup -c ./build/config.dev.js",
    "prod": "node_modules/.bin/rollup -c ./build/config.prod.js",
    "pure": "node_modules/.bin/rollup -c ./build/config.pure.js",
    "runtime": "node_modules/.bin/rollup -c ./build/config.runtime.js",
    "build": "npm run pure && npm run runtime && npm run prod && npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yoxjs/yox.git"
  },
  "keywords": [
    "easy",
    "simple",
    "lightweight",
    "mvvm",
    "framework",
    "vue"
  ],
  "author": "musicode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yoxjs/yox/issues"
  },
  "homepage": "https://github.com/yoxjs/yox#readme",
  "devDependencies": {
    "rollup": "^1.10.1",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript": "^1.0.1",
    "tslib": "^1.9.3",
    "typescript": "^3.4.4"
  },
  "dependencies": {
    "buble": "^0.19.7",
    "yox-common": "^0.18.27",
    "yox-config": "^0.1.6",
    "yox-dom": "^0.0.24",
    "yox-observer": "^0.17.26",
    "yox-snabbdom": "^0.19.27",
    "yox-template-compiler": "^0.27.40",
    "yox-type": "^0.0.40"
  }
}
