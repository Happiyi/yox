{
  "name": "yox",
  "version": "1.0.0-alpha.45",
  "description": "A lightweight mvvm framework",
  "main": "dist/standard/dev/yox.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "doc": "docsify serve docs",

    "dev": "node_modules/.bin/rollup -c ./build/config.dev.js",

    "dev-standard-source": "node_modules/.bin/rollup -c ./build/config.dev.standard.source.js",
    "prod-standard-source": "node_modules/.bin/rollup -c ./build/config.prod.standard.source.js",
    "runtime-standard-source": "node_modules/.bin/rollup -c ./build/config.runtime.standard.source.js",
    "pure-standard-source": "node_modules/.bin/rollup -c ./build/config.pure.standard.source.js",

    "dev-standard-min": "node_modules/.bin/rollup -c ./build/config.dev.standard.min.js",
    "prod-standard-min": "node_modules/.bin/rollup -c ./build/config.prod.standard.min.js",
    "runtime-standard-min": "node_modules/.bin/rollup -c ./build/config.runtime.standard.min.js",
    "pure-standard-min": "node_modules/.bin/rollup -c ./build/config.pure.standard.min.js",

    "dev-legacy-source": "node_modules/.bin/rollup -c ./build/config.dev.legacy.source.js",
    "prod-legacy-source": "node_modules/.bin/rollup -c ./build/config.prod.legacy.source.js",
    "runtime-legacy-source": "node_modules/.bin/rollup -c ./build/config.runtime.legacy.source.js",
    "pure-legacy-source": "node_modules/.bin/rollup -c ./build/config.pure.legacy.source.js",

    "dev-legacy-min": "node_modules/.bin/rollup -c ./build/config.dev.legacy.min.js",
    "prod-legacy-min": "node_modules/.bin/rollup -c ./build/config.prod.legacy.min.js",
    "runtime-legacy-min": "node_modules/.bin/rollup -c ./build/config.runtime.legacy.min.js",
    "pure-legacy-min": "node_modules/.bin/rollup -c ./build/config.pure.legacy.min.js",

    "build": "npm run pure-standard-source && npm run runtime-standard-source && npm run prod-standard-source && npm run dev-standard-source && npm run pure-legacy-source && npm run runtime-legacy-source && npm run prod-legacy-source && npm run dev-legacy-source && npm run pure-standard-min && npm run runtime-standard-min && npm run prod-standard-min && npm run dev-standard-min && npm run pure-legacy-min && npm run runtime-legacy-min && npm run prod-legacy-min && npm run dev-legacy-min"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yoxjs/yox.git"
  },
  "keywords": [
    "easy",
    "simple",
    "lightweight",
    "mvvm",
    "typescript",
    "framework",
    "vue"
  ],
  "author": "musicode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yoxjs/yox/issues"
  },
  "homepage": "https://github.com/yoxjs/yox#readme",
  "devDependencies": {
    "rollup": "^1.10.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript": "^1.0.1",
    "tslib": "^1.9.3",
    "typescript": "^3.4.4"
  },
  "dependencies": {

  }
}
