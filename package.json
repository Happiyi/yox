{
  "name": "yox",
  "version": "1.0.0-alpha.27",
  "description": "A lightweight mvvm framework",
  "main": "dist/standard/dev/yox.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "doc": "docsify serve docs",
    "dev": "node_modules/.bin/rollup -c ./build/config.dev.js",
    "prod": "node_modules/.bin/rollup -c ./build/config.prod.js",
    "runtime": "node_modules/.bin/rollup -c ./build/config.runtime.js",
    "pure": "node_modules/.bin/rollup -c ./build/config.pure.js",
    "build": "npm run pure && npm run runtime && npm run prod && npm run dev",
    "dev-legacy": "node_modules/.bin/rollup -c ./build/config.dev.legacy.js",
    "prod-legacy": "node_modules/.bin/rollup -c ./build/config.prod.legacy.js",
    "runtime-legacy": "node_modules/.bin/rollup -c ./build/config.runtime.legacy.js",
    "pure-legacy": "node_modules/.bin/rollup -c ./build/config.pure.legacy.js",
    "build-legacy": "npm run pure-legacy && npm run runtime-legacy && npm run prod-legacy && npm run dev-legacy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yoxjs/yox.git"
  },
  "keywords": [
    "easy",
    "simple",
    "lightweight",
    "mvvm",
    "typescript",
    "framework",
    "vue"
  ],
  "author": "musicode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yoxjs/yox/issues"
  },
  "homepage": "https://github.com/yoxjs/yox#readme",
  "devDependencies": {
    "rollup": "^1.10.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript": "^1.0.1",
    "tslib": "^1.9.3",
    "typescript": "^3.4.4"
  },
  "dependencies": {
    "yox-common": "^0.18.47",
    "yox-config": "^0.1.8",
    "yox-dom": "^0.0.43",
    "yox-observer": "^0.17.41",
    "yox-snabbdom": "^0.19.43",
    "yox-template-compiler": "^0.27.68",
    "yox-type": "^0.0.64"
  }
}
